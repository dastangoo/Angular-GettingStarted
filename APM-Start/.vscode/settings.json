// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.map": true,
        "**/app/**/*.js": true
    },
    // Controls auto save of dirty files.
    "files.autoSave": "afterDelay"
}